  "z-index": {
    "hide": {
      "$type": "number",
      "$value": "-1",
      "$description": "Used to visually hide elements beneath the base layer—commonly for offscreen or visually hidden UI"
    },
    "base": {
      "$type": "number",
      "$value": "0",
      "$description": "The default stacking context for basic content—acts as the baseline"
    },
    "docked": {
      "$type": "number",
      "$value": "10",
      "$description": "For fixed-position UI elements (like toolbars or docked panels) that should sit above base content but below overlays; docked elements shouldn’t interfere with content layering"
