      "$value": "1200",
      "$description": "For persistent banners or notifications that overlap sticky headers"
    },
    "overlay": {
      "$type": "number",
      "$value": "1300",
      "$description": "Used for semi-transparent overlays behind dialogs or modals"
    },
    "modal": {
      "$type": "number",
      "$value": "1400",
      "$description": "Assigned to modal dialogs themselves, placing them above overlays"
    },
    "popover": {
      "$type": "number",
